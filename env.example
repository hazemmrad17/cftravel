# ASIA.fr Travel Agent Environment Configuration
# Copy this file to .env and fill in your actual values

# Groq API Configuration (Required)
GROQ_API_KEY=your_groq_api_key_here

# Model Configuration (Optional - uses defaults if not set)
REASONING_MODEL=deepseek-r1-distill-llama-70b
GENERATION_MODEL=llama-3.1-8b-instant
MATCHER_MODEL=llama-3.1-8b-instant
EXTRACTOR_MODEL=llama-3.1-8b-instant

# Model Temperature Settings (Optional - uses defaults if not set)
REASONING_TEMPERATURE=0.1
GENERATION_TEMPERATURE=0.7
MATCHER_TEMPERATURE=0.3
EXTRACTOR_TEMPERATURE=0.1

# Model Max Tokens Settings (Optional - uses defaults if not set)
REASONING_MAX_TOKENS=2048
GENERATION_MAX_TOKENS=2048
MATCHER_MAX_TOKENS=2048
EXTRACTOR_MAX_TOKENS=1024

# Embedding Model (Optional - uses default if not set)
EMBEDDING_MODEL=all-MiniLM-L6-v2

# API Configuration (Optional - uses defaults if not set)
AGENT_API_URL=http://localhost:8001
GROQ_BASE_URL=https://api.groq.com/openai/v1

# Debug Mode (Optional)
DEBUG=false

# Data Path (Optional - uses default if not set)
DATA_PATH=./cftravel_py/data 