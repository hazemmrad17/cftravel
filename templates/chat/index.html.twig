				{% set today = date('today') %}
				{% set yesterday = date('yesterday') %}
				{% set sevenDaysAgo = date('-7 days') %}
				{% set conversationsToday = [] %}
				{% set conversationsYesterday = [] %}
				{% set conversationsPrevious = [] %}
				{% for conv in conversations %}
					{% set startedAt = conv.startedAt %}
					{% if startedAt >= today %}
						{% set conversationsToday = conversationsToday|merge([conv]) %}
					{% elseif startedAt >= yesterday and startedAt < today %}
						{% set conversationsYesterday = conversationsYesterday|merge([conv]) %}
					{% elseif startedAt >= sevenDaysAgo and startedAt < yesterday %}
						{% set conversationsPrevious = conversationsPrevious|merge([conv]) %}
					{% endif %}
				{% endfor %}
				{% include 'chat/_sidebar.html.twig' with {
				  'conversationsToday': conversationsToday,
				  'conversationsYesterday': conversationsYesterday,
				  'conversationsPrevious': conversationsPrevious,
