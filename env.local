# Local Development Environment Configuration
ENVIRONMENT=local

# API Configuration
API_HOST=0.0.0.0
API_PORT=8002
AGENT_API_URL=http://localhost:8002

# Groq API Configuration (Required)
GROQ_API_KEY=your_groq_api_key_here

# Model Configuration - Kimia K2 for French responses
REASONING_MODEL=openai/gpt-oss-120b
GENERATION_MODEL=moonshotai/kimi-k2-instruct
MATCHER_MODEL=moonshotai/kimi-k2-instruct
EXTRACTOR_MODEL=moonshotai/kimi-k2-instruct

# Model Temperature Settings - Optimized for French responses
REASONING_TEMPERATURE=0.1
GENERATION_TEMPERATURE=0.7
MATCHER_TEMPERATURE=0.3
EXTRACTOR_TEMPERATURE=0.1

# Model Max Tokens Settings
REASONING_MAX_TOKENS=2048
GENERATION_MAX_TOKENS=2048
MATCHER_MAX_TOKENS=2048
EXTRACTOR_MAX_TOKENS=1024

# Embedding Model
EMBEDDING_MODEL=all-MiniLM-L6-v2

# Groq Base URL
GROQ_BASE_URL=https://api.groq.com/openai/v1

# Language Configuration
LANGUAGE=fr
RESPOND_IN_FRENCH=true

# Debug Mode
DEBUG=true

# Data Path
DATA_PATH=./cftravel_py/data 